          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">// Get account numbers from properties
String sourceAcct = props.get("sourceAccountNumber");
String destAcct = props.get("destAccountNumber");

// Set them as variables for the current thread
vars.put("sourceAccountNumber", sourceAcct);
vars.put("destAccountNumber", destAcct);

// Generate random amount between 1 and 100
double amount = 1 + Math.random() * 99;
// Format to 2 decimal places
String formattedAmount = String.format("%.2f", amount);
vars.put("transferAmount", formattedAmount);</stringProp>
